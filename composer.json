{
  "name": "wwsh/gastro-api",
  "description": "Maintained Gastro API for desktop and mobile apps",
  "keywords": [
    "psr",
    "psr-3"
  ],
  "license": "GPL",
  "authors": [
    {
      "name": "Thomas Parys",
      "homepage": "http://wwsh.io/"
    }
  ],
  "autoload": {
    "psr-0": {
      "" : "src"
    }
  },
  "require": {
    "php": ">=5.4",
    "symfony/symfony": ">=2.8",
    "symfony/monolog-bundle": ">=2.7",
    "doctrine/doctrine-bundle": ">=1.4",
    "doctrine/orm": "~2.5,>=2.2.3",
    "doctrine/mongodb": "1.2.*@dev",
    "doctrine/mongodb-odm": "1.0.*@dev",
    "doctrine/mongodb-odm-bundle": "3.0.*@dev",
    "twig/extensions": "~1.2",
    "sensio/distribution-bundle": "*",
    "sensio/framework-extra-bundle": "3.*",
    "incenteev/composer-parameter-handler": "~2.1",
    "friendsofsymfony/rest-bundle": "1.7.7",
    "nelmio/api-doc-bundle": "^2.11",
    "jms/security-extra-bundle": "^1.5",
    "jms/di-extra-bundle": "1.*",
    "qandidate/symfony-json-request-transformer": "1.*",
    "knplabs/knp-snappy-bundle": "^1.2",
    "jms/serializer-bundle": "^1.1"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.0",
    "mockery/mockery": "*"
  },
  "config": {
    "bin-dir": "bin"
  }
}
